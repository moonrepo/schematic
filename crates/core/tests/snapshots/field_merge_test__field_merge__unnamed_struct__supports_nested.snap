fn merge(
    &mut self,
    context: &Self::Context,
    mut next: Self,
) -> std::result::Result<(), schematic::ConfigError> {
    use schematic::internal::*;
    MergeManager::new(context)
        .nested(&mut self.0, next.0)?
        .nested(&mut self.1, next.1)?
        .nested(&mut self.2, next.2)?
        .nested(&mut self.3, next.3)?;
    Ok(())
}
