fn env_values() -> std::result::Result<Option<Self>, schematic::ConfigError> {
    use schematic::internal::*;
    let mut env = EnvManager::default();
    let mut partial = Self::default();
    partial.a = env.get("A")?;
    partial.b = env.get("B")?;
    partial.c = env.get("C")?;
    partial.d = env.get("D")?;
    Ok(if env.is_empty() { None } else { Some(partial) })
}
