fn merge(
    &mut self,
    context: &Self::Context,
    mut next: Self,
) -> std::result::Result<(), schematic::ConfigError> {
    use schematic::internal::*;
    MergeManager::new(context)
        .nested(&mut self.a, next.a)?
        .nested(&mut self.b, next.b)?
        .nested(&mut self.c, next.c)?
        .nested(&mut self.d, next.d)?;
    Ok(())
}
